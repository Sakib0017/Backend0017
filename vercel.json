{
    "version": 2,
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node"
      },
      { 
       "src": "api/routers/ComplaintSubmissionRouter.js",
       "use": "@vercel/node" 
      },
      { 
       "src": "api/routers/ContactRouter.js",
       "use": "@vercel/node" 
      },
      { 
       "src": "api/routers/SampleCollectionRouter.js",
       "use": "@vercel/node" 
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/routers/ComplaintSubmissionRouter.js"
      }
    ]
   }
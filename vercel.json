{
    "version": 2,
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node"
      },
      { 
       "src": "src/api/routers/ComplaintSubmissionRouter.js",
       "use": "@vercel/node" 
      },
      { 
       "src": "src/api/routers/ContactRouter.js",
       "use": "@vercel/node" 
      },
      { 
       "src": "src/api/routers/SampleCollectionRouter.js",
       "use": "@vercel/node" 
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/server.js"
      },
      { 
        "src": "src/api/(.*)", 
        "dest": "src/api/sample-collections" 
      }
    ]
   }
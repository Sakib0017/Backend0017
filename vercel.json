{
    "version": 2,
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node"
      },
      { 
       "src": "src/app.js",
       "use": "@vercel/node" 
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/server.js"
      },
      { 
        "src": "/(.*)", 
        "dest": "src/app.js" 
      }
    ]
   }
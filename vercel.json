{
    "version": 2,
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/server.js"
      },
      {
        "src": "/api/messages",
        "dest": "/"
      },
      {
        "src": "/api/complaints",
        "dest": "/"
      },
      {
        "src": "/api/sample-collections",
        "dest": "/"
      }
    ]
  }
{
    "version": 2,
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/server.js"
      },
      {
        "src": "src/app.js",
        "dest": "/"
      }
    ],
    "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }

    
  }
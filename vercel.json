{
    "version": 2,
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/server.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "src/server.js"
      }
    ],
    "functions" : {
      "api/app.js": {
      "maxDuration":30
    }
    }

    
  }